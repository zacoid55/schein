{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ES2017"
    ],
    "rootDir": "./",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "module": "ESNext",
    "types": [
      "cypress",
      "./cypress/support/index.d.ts"
    ]
  },
  "include": [
    "**/*.ts",
    "./cypress/cypress-webpack.config.js",
    "./cypress.config.ts"
  ]
}